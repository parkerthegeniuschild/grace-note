{
    "name": "grace-note",
    "version": "1.0.0",
    "description": "",
    "main": "./src/index.js",
    "scripts": {
        "clear:artefacts": "rm -rf tmp logs dist",
        "build": "babel ./src -d ./dist --copy-files",
        "dev": "cross-env NODE_ENV=development nodemon --exec babel-node ./src/index.js",
        "start": "cross-env NODE_ENV=production node ./dist/index.js",
        "lint": "eslint src --fix",
        "docker:build": "docker build . -t grace-note:latest",
        "docker:run": "docker run --name grace-note -d -p 5000:5000 --env-file .env grace-note:latest"
    },
