    "dependencies": {
        "@hapi/joi": "^17.1.1",
        "app-root-path": "^3.0.0",
        "axios": "^0.22.0",
        "body-parser": "^1.19.0",
        "case": "^1.6.3",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "dayjs": "^1.10.7",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
